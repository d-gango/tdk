\begin_layout Chapter
System matrices of the 5 DOF space robot model
\begin_inset CommandInset label
LatexCommand label
name "chap:System-matrices-of-5dof"

\end_inset


\end_layout

\begin_layout Standard
The inertia matrix is
\begin_inset Formula 
\begin{equation}
\bm{M}=\left[\begin{array}{ccccc}
m_{11} & m_{12} & m_{13} & m_{14} & m_{15}\\
m_{21} & m_{22} & m_{23} & m_{24} & m_{25}\\
m_{31} & m_{32} & m_{33} & m_{34} & m_{35}\\
m_{41} & m_{42} & m_{43} & m_{44} & m_{45}\\
m_{51} & m_{52} & m_{53} & m_{54} & m_{55}
\end{array}\right],
\end{equation}

\end_inset

where the elements of the matrix are the following:
\begin_inset Formula 
\begin{equation}
m_{11}=m_{0}+m_{1}+m_{2},
\end{equation}

\end_inset


\begin_inset Formula 
\begin{equation}
m_{12}=0,
\end{equation}

\end_inset


\begin_inset Formula 
\begin{equation}
m_{13}=-l_{0}\sin\left(\varphi_{0}+q_{0}\right)\left(m_{1}+m_{2}\right),
\end{equation}

\end_inset


\begin_inset Formula 
\begin{equation}
m_{14}=-l_{1}\sin\left(q_{1}\right)\left(\frac{m_{1}}{2}+m_{2}\right),
\end{equation}

\end_inset


\begin_inset Formula 
\begin{equation}
m_{15}=-\frac{l_{2}m_{2}\sin\left(q_{2}\right)}{2},
\end{equation}

\end_inset


\begin_inset Formula 
\begin{equation}
m_{21}=0,
\end{equation}

\end_inset


\begin_inset Formula 
\begin{equation}
m_{22}=m_{0}+m_{1}+m_{2},
\end{equation}

\end_inset


\begin_inset Formula 
\begin{equation}
m_{23}=l_{0}\cos\left(\varphi_{0}+q_{0}\right)\left(m_{1}+m_{2}\right),
\end{equation}

\end_inset


\begin_inset Formula 
\begin{equation}
m_{24}=l_{1}\cos\left(q_{1}\right)\left(\frac{m_{1}}{2}+m_{2}\right),
\end{equation}

\end_inset


\begin_inset Formula 
\begin{equation}
m_{25}=\frac{l_{2}m_{2}\cos\left(q_{2}\right)}{2},
\end{equation}

\end_inset


\begin_inset Formula 
\begin{equation}
m_{31}=-l_{0}\sin\left(\varphi_{0}+q_{0}\right)\left(m_{1}+m_{2}\right),
\end{equation}

\end_inset


\begin_inset Formula 
\begin{equation}
m_{32}=l_{0}\cos\left(\varphi_{0}+q_{0}\right)\left(m_{1}+m_{2}\right),
\end{equation}

\end_inset


\begin_inset Formula 
\begin{equation}
m_{33}=\left(m_{1}+m_{2}\right)l_{0}^{2}+\theta_{0}+\theta_{1}+\theta_{2},
\end{equation}

\end_inset


\begin_inset Formula 
\begin{equation}
m_{34}=\theta_{1}+\theta_{2}+l_{0}l_{1}\cos\left(\varphi_{0}+q_{0}-q_{1}\right)\left(\frac{m_{1}}{2}+m_{2}\right),
\end{equation}

\end_inset


\begin_inset Formula 
\begin{equation}
m_{35}=\theta_{2}+\frac{l_{0}l_{2}m_{2}\cos\left(\varphi_{0}+q_{0}-q_{2}\right)}{2},
\end{equation}

\end_inset


\begin_inset Formula 
\begin{equation}
m_{41}=-l_{1}\sin\left(q_{1}\right)\left(\frac{m_{1}}{2}+m_{2}\right),
\end{equation}

\end_inset


\begin_inset Formula 
\begin{equation}
m_{42}=l_{1}\cos\left(q_{1}\right)\left(\frac{m_{1}}{2}+m_{2}\right),
\end{equation}

\end_inset


\begin_inset Formula 
\begin{equation}
m_{43}=\theta_{1}+\theta_{2}+l_{0}l_{1}\cos\left(\varphi_{0}+q_{0}-q_{1}\right)\left(\frac{m_{1}}{2}+m_{2}\right),
\end{equation}

\end_inset


\begin_inset Formula 
\begin{equation}
m_{44}=\left(\frac{m_{1}}{4}+m_{2}\right)l_{1}^{2}+\theta_{1}+\theta_{2},
\end{equation}

\end_inset


\begin_inset Formula 
\begin{equation}
m_{45}=\theta_{2}+\frac{l_{1}l_{2}m_{2}\cos\left(q_{1}-q_{2}\right)}{2},
\end{equation}

\end_inset


\begin_inset Formula 
\begin{equation}
m_{51}=-\frac{l_{2}m_{2}\sin\left(q_{2}\right)}{2},
\end{equation}

\end_inset


\begin_inset Formula 
\begin{equation}
m_{52}=\frac{l_{2}m_{2}\cos\left(q_{2}\right)}{2},
\end{equation}

\end_inset


\begin_inset Formula 
\begin{equation}
m_{53}=\theta_{2}+\frac{l_{0}l_{2}m_{2}\cos\left(\varphi_{0}+q_{0}-q_{2}\right)}{2},
\end{equation}

\end_inset


\begin_inset Formula 
\begin{equation}
m_{54}=\theta_{2}+\frac{l_{1}l_{2}m_{2}\cos\left(q_{1}-q_{2}\right)}{2},
\end{equation}

\end_inset


\begin_inset Formula 
\begin{equation}
m_{55}=\frac{m_{2}l_{2}^{2}}{4}+\theta_{2}.
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
The vector of Coriolis and centripetal torques is 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\bm{C}=\left[\begin{array}{c}
c_{11}\\
c_{12}\\
c_{13}\\
c_{14}\\
c_{15}
\end{array}\right].
\end{equation}

\end_inset

Where 
\begin_inset Formula 
\begin{equation}
c_{11}=-l_{0}\cos\left(\varphi_{0}+q_{0}\right)\left(m_{1}+m_{2}\right)\mathrm{\dot{q}}_{0}^{2}-l_{1}\cos\left(q_{1}\right)\left(\frac{m_{1}}{2}+m_{2}\right)\mathrm{\dot{q}}_{1}^{2}-\frac{l_{2}m_{2}\cos\left(q_{2}\right)\mathrm{\dot{q}}_{2}^{2}}{2},
\end{equation}

\end_inset


\begin_inset Formula 
\begin{equation}
c_{12}=-l_{0}\sin\left(\varphi_{0}+q_{0}\right)\left(m_{1}+m_{2}\right)\mathrm{\dot{q}}_{0}^{2}-l_{1}\sin\left(q_{1}\right)\left(\frac{m_{1}}{2}+m_{2}\right)\mathrm{\dot{q}}_{1}^{2}-\frac{l_{2}m_{2}\sin\left(q_{2}\right)\mathrm{\dot{q}}_{2}^{2}}{2},
\end{equation}

\end_inset


\begin_inset Formula 
\begin{equation}
c_{13}=l_{0}\left(l_{1}\sin\left(\varphi_{0}+q_{0}-q_{1}\right)\left(\frac{m_{1}}{2}+m_{2}\right)\mathrm{\dot{q}}_{1}^{2}+\frac{l_{2}m_{2}\sin\left(\varphi_{0}+q_{0}-q_{2}\right)\mathrm{\dot{q}}_{2}^{2}}{2}\right),
\end{equation}

\end_inset


\begin_inset Formula 
\begin{equation}
c_{14}=l_{1}\left(\frac{\mathrm{\dot{q}}_{2}^{2}l_{2}m_{2}\sin\left(q_{1}-q_{2}\right)}{2}-\mathrm{\dot{q}}_{0}^{2}l_{0}\sin\left(\varphi_{0}+q_{0}-q_{1}\right)\left(\frac{m_{1}}{2}+m_{2}\right)\right),
\end{equation}

\end_inset


\begin_inset Formula 
\begin{equation}
c_{15}=-l_{2}m_{2}\left(\frac{l_{0}\sin\left(\varphi_{0}+q_{0}-q_{2}\right)\mathrm{\dot{q}}_{0}^{2}}{2}+\frac{l_{1}\sin\left(q_{1}-q_{2}\right)\mathrm{\dot{q}}_{1}^{2}}{2}\right).
\end{equation}

\end_inset


\end_layout

\begin_layout Chapter
System matrices of the 3 DOF space robot model
\begin_inset CommandInset label
LatexCommand label
name "chap:System-matrices-of-3dof"

\end_inset


\end_layout

\begin_layout Standard
The inertia matrix is
\begin_inset Formula 
\begin{equation}
\bm{M}=\left[\begin{array}{ccc}
m_{11} & m_{12} & m_{13}\\
m_{21} & m_{22} & m_{23}\\
m_{31} & m_{32} & m_{33}
\end{array}\right],
\end{equation}

\end_inset

where the elements of the matrix are the following:
\begin_inset Formula 
\begin{equation}
m_{11}=I_{0}+I_{1}+I_{2}+\frac{l_{0}^{2}m_{0}\left(m_{1}+m_{2}\right)}{m_{0}+m_{1}+m_{2}},
\end{equation}

\end_inset


\begin_inset Formula 
\begin{equation}
\begin{array}{c}
m_{12}=\frac{\left(I_{1}+I_{2}\right)\left(m_{0}^{2}+2m_{0}m_{1}+2m_{0}m_{2}+m_{1}^{2}+2m_{1}m_{2}+m_{2}^{2}\right)+}{\left(m_{0}+m_{1}+m_{2}\right)^{2}}\\
\frac{+l_{0}l_{1}m_{0}\cos\left(\varphi_{0}+q_{0}-q_{1}\right)\left(\frac{m_{1}^{2}}{2}+\frac{3m_{1}m_{2}}{2}+\frac{m_{0}m_{1}}{2}+m_{2}^{2}+m_{0}m_{2}\right)}{\left(m_{0}+m_{1}+m_{2}\right)^{2}},
\end{array}
\end{equation}

\end_inset


\begin_inset Formula 
\begin{equation}
\begin{array}{c}
m_{13}=\frac{I_{2}\left(m_{0}^{2}+2m_{0}m_{1}+2m_{0}m_{2}+m_{1}^{2}+2m_{1}m_{2}+m_{2}^{2}\right)+}{\left(m_{0}+m_{1}+m_{2}\right)^{2}}\\
\frac{+l_{0}l_{2}m_{0}m_{2}\cos\left(\varphi_{0}+q_{0}-q_{2}\right)\left(\frac{m_{0}}{2}+\frac{m_{1}}{2}+\frac{m_{2}}{2}\right)}{\left(m_{0}+m_{1}+m_{2}\right)^{2}},
\end{array}
\end{equation}

\end_inset


\begin_inset Formula 
\begin{equation}
\begin{array}{c}
m_{21}=\frac{\left(I_{1}+I_{2}\right)\left(m_{0}^{2}+2m_{0}m_{1}+2m_{0}m_{2}+m_{1}^{2}+2m_{1}m_{2}+m_{2}^{2}\right)+}{\left(m_{0}+m_{1}+m_{2}\right)^{2}}\\
\frac{+l_{0}l_{1}m_{0}\cos\left(\varphi_{0}+q_{0}-q_{1}\right)\left(\frac{m_{1}^{2}}{2}+\frac{3m_{1}m_{2}}{2}+\frac{m_{0}m_{1}}{2}+m_{2}^{2}+m_{0}m_{2}\right)}{\left(m_{0}+m_{1}+m_{2}\right)^{2}},
\end{array}
\end{equation}

\end_inset


\begin_inset Formula 
\begin{equation}
m_{22}=I_{1}+I_{2}+\frac{l_{1}^{2}\left(\frac{m_{0}m_{1}}{4}+m_{0}m_{2}+\frac{m_{1}m_{2}}{4}\right)}{m_{0}+m_{1}+m_{2}},
\end{equation}

\end_inset


\begin_inset Formula 
\begin{equation}
\begin{array}{c}
m_{23}=\frac{I_{2}\left(m_{0}^{2}+2m_{0}m_{1}+2m_{0}m_{2}+m_{1}^{2}+2m_{1}m_{2}+m_{2}^{2}\right)+}{\left(m_{0}+m_{1}+m_{2}\right)^{2}}\\
\frac{+l_{1}l_{2}m_{2}\cos\left(q_{1}-q_{2}\right)\left(\frac{m_{0}^{2}}{2}+\frac{3m_{0}m_{1}}{4}+\frac{m_{2}m_{0}}{2}+\frac{m_{1}^{2}}{4}+\frac{m_{2}m_{1}}{4}\right)}{\left(m_{0}+m_{1}+m_{2}\right)^{2}},
\end{array}
\end{equation}

\end_inset


\begin_inset Formula 
\begin{equation}
\begin{array}{c}
m_{31}=\frac{I_{2}\left(m_{0}^{2}+2m_{0}m_{1}+2m_{0}m_{2}+m_{1}^{2}+2m_{1}m_{2}+m_{2}^{2}\right)+}{\left(m_{0}+m_{1}+m_{2}\right)^{2}}\\
\frac{+l_{0}l_{2}m_{0}m_{2}\cos\left(\varphi_{0}+q_{0}-q_{2}\right)\left(\frac{m_{0}}{2}+\frac{m_{1}}{2}+\frac{m_{2}}{2}\right)}{\left(m_{0}+m_{1}+m_{2}\right)^{2}},
\end{array}
\end{equation}

\end_inset


\begin_inset Formula 
\begin{equation}
\begin{array}{c}
m_{32}=\frac{I_{2}\left(m_{0}^{2}+2m_{0}m_{1}+2m_{0}m_{2}+m_{1}^{2}+2m_{1}m_{2}+m_{2}^{2}\right)+}{\left(m_{0}+m_{1}+m_{2}\right)^{2}}\\
\frac{+l_{1}l_{2}m_{2}\cos\left(q_{1}-q_{2}\right)\left(\frac{m_{0}^{2}}{2}+\frac{3m_{0}m_{1}}{4}+\frac{m_{2}m_{0}}{2}+\frac{m_{1}^{2}}{4}+\frac{m_{2}m_{1}}{4}\right)}{\left(m_{0}+m_{1}+m_{2}\right)^{2}},
\end{array}
\end{equation}

\end_inset


\begin_inset Formula 
\begin{equation}
m_{33}=I_{2}+\frac{l_{2}^{2}m_{2}\left(\frac{m_{0}}{4}+\frac{m_{1}}{4}\right)}{m_{0}+m_{1}+m_{2}}.
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
The vector of Coriolis and centripetal torques is 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\bm{C}=\left(\begin{array}{c}
c_{11}\\
c_{12}\\
c_{13}
\end{array}\right).
\end{equation}

\end_inset

Where
\begin_inset Formula 
\begin{equation}
\begin{array}{c}
c_{11}=\frac{l_{0}m_{0}l_{1}\sin\left(\varphi_{0}+q_{0}-q_{1}\right)\left(\frac{m_{1}^{2}}{2}+\frac{3m_{1}m_{2}}{2}+\frac{m_{0}m_{1}}{2}+m_{2}^{2}+m_{0}m_{2}\right)\dot{q}_{2}^{2}+}{\left(m_{0}+m_{1}+m_{2}\right)^{2}}\\
\frac{+l_{0}m_{0}l_{2}m_{2}\sin\left(\varphi_{0}+q_{0}-q_{2}\right)\left(\frac{m_{0}}{2}+\frac{m_{1}}{2}+\frac{m_{2}}{2}\right)\dot{q}_{3}^{2}}{\left(m_{0}+m_{1}+m_{2}\right)^{2}},
\end{array}
\end{equation}

\end_inset


\begin_inset Formula 
\begin{equation}
\begin{array}{c}
c_{12}=\frac{l_{1}\dot{q}_{3}^{2}l_{2}m_{2}\sin\left(q_{1}-q_{2}\right)\left(\frac{m_{0}^{2}}{2}+\frac{3m_{0}m_{1}}{4}+\frac{m_{2}m_{0}}{2}+\frac{m_{1}^{2}}{4}+\frac{m_{2}m_{1}}{4}\right)-}{\left(m_{0}+m_{1}+m_{2}\right)^{2}}\\
\frac{-l_{1}\dot{q}_{1}^{2}l_{0}m_{0}\sin\left(\varphi_{0}+q_{0}-q_{1}\right)\left(\frac{m_{1}^{2}}{2}+\frac{3m_{1}m_{2}}{2}+\frac{m_{0}m_{1}}{2}+m_{2}^{2}+m_{0}m_{2}\right)}{\left(m_{0}+m_{1}+m_{2}\right)^{2}},
\end{array}
\end{equation}

\end_inset


\begin_inset Formula 
\begin{equation}
\begin{array}{c}
c_{13}=-\frac{l_{2}m_{2}l_{0}m_{0}\sin\left(\varphi_{0}+q_{0}-q_{2}\right)\left(\frac{m_{0}}{2}+\frac{m_{1}}{2}+\frac{m_{2}}{2}\right)\dot{q}_{1}^{2}+}{\left(m_{0}+m_{1}+m_{2}\right)^{2}}\\
-\frac{+l_{2}m_{2}l_{1}\sin\left(q_{1}-q_{2}\right)\left(\frac{m_{0}^{2}}{2}+\frac{3m_{0}m_{1}}{4}+\frac{m_{2}m_{0}}{2}+\frac{m_{1}^{2}}{4}+\frac{m_{2}m_{1}}{4}\right)\dot{q}_{2}^{2}}{\left(m_{0}+m_{1}+m_{2}\right)^{2}}.
\end{array}
\end{equation}

\end_inset


\end_layout

